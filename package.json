{
  "name": "nativescript-environment-hook",
  "version": "1.0.0",
  "description": "Set environment info to json",
  "nativescript": {
    "hooks": [
      {
        "type": "before-prepare",
        "script": "lib/before-prepare.js",
        "inject": true
      }
